{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node_modules/.bin/jest",
    "e2e": "detox test",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand"
  },
  "jest": {
    "preset": "jest-expo",
    "testMatch": [
      "<rootDir>/__tests__/*.js?(x)"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "bin/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    },
    "test-runner": "jest"
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "@expo/vector-icons": "^10.0.0",
    "@unimodules/react-native-adapter": "^2.0.1",
    "axios": "^0.19.0",
    "expo": "^33.0.7",
    "expo-font": "~5.0.1",
    "form-data": "^2.5.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-animated-ellipsis": "^2.0.0",
    "react-native-elements": "^1.1.0",
    "react-native-expo-viewport-units": "0.0.8",
    "react-native-fullwidth-image": "^0.1.3",
    "react-native-navigation": "^2.12.0",
    "react-native-progress": "^3.6.0",
    "react-navigation": "^3.0.9",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/react-native": "^0.60.2",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-expo": "^5.0.0",
    "detox": "^12.3.0",
    "detox-expo-helpers": "^0.6.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-airbnb-typescript-prettier": "^1.2.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.2",
    "expo-detox-hook": "^1.0.10",
    "jest-expo": "^33.0.2",
    "prettier": "^1.17.0",
    "react-native-dotenv": "^0.2.0",
    "typescript": "^3.5.3"
  },
  "private": true
}
